{
  "files.exclude": {
    ".husky": false,
    "node_modules": false,
    "bin": false,
    ".github": false
  },
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "jest.autoRun": "off",
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  }
}
